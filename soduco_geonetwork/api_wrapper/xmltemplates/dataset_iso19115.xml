   <mdb:resourceLineage>
      <mrl:LI_Lineage>
      </mrl:LI_Lineage>
   </mdb:resourceLineage>
   <mdb:metadataScope>
      <mdb:MD_MetadataScope>
         <mdb:resourceScope>
            <mcc:MD_ScopeCode codeList="http://standards.iso.org/iso/19115/resources/Codelists/cat/codelists.xml#MD_ScopeCode"
                              codeListValue="dataset"/>
         </mdb:resourceScope>
      </mdb:MD_MetadataScope>
   </mdb:metadataScope>
